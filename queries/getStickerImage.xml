<query id="getStickerImage" action="select">
	<tables>
		<table name="sticker_files" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="sticker_srl" var="sticker_srl" filter="number" notnull="notnull" />
	</conditions>
	<navigation>
		<index var="sort_index" default="no" order="asc" />
	</navigation>
</query>

<query id="getStickerMainImage" action="select">
	<tables>
		<table name="sticker_files" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="sticker_srl" var="sticker_srl" notnull="notnull" />
		<condition operation="equal" column="no" default="0" var="no" pipe="and" />
	</conditions>
</query>

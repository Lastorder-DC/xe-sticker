<query id="getStickerFileByStickerFileSrl" action="select">
	<tables>
		<table name="sticker_files" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="sticker_file_srl" var="sticker_file_srl" filter="number" notnull="notnull" />
	</conditions>
</query>

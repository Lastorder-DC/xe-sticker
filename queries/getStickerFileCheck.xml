<query id="getStickerFileCheck" action="select">
	<tables>
		<table name="sticker_files" />
	</tables>
	<columns>
      <column name="count(*)" alias="count" />
	</columns>
	<conditions>
		<condition operation="equal" column="sticker_srl" var="sticker_srl" filter="number" notnull="notnull" />
	</conditions>
</query>
